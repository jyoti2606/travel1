<mvc:View

    xmlns:form="sap.ui.layout.form"

    controllerName="probasicfiori.controller.View2"

  

    xmlns:mvc="sap.ui.core.mvc"

    displayBlock="true"

    xmlns:core="sap.ui.core"

    xmlns="sap.m"

>

<Page id="_IDGenPage156" title="Travel Detail">

 <Panel id="_IDGenPanel1" >

  <Shell id="_IDGenShell1" >

    <form:SimpleForm

        id="_IDGenSimpleForm1"

        editable="true"

    >

     <core:Title text="" />

        <Label

            id="_IDGenLabel1"

            text="Travel type"

            required="true"

            design="Bold"

        />

        <Select

            id="country"

            selectedKey="{Country}"

           

            width="150px"

        >

            <items>

                <core:Item id="ui6" text="select" />

                <core:Item id="_IDGenItem1" text="Domestic" />

                <core:Item id="_IDGenItem2" text="International" />

            </items>

        </Select>

        <Label id="_IDGenLabel4" text=""></Label>

        <Text id="_IDGenText1" text=""></Text>

           

        <Label

            id="_IDGenLabel2"

             design="Bold"

             required="true"

            text="Number of Passenger"

        />

        <Input id="in8"

            value=""

            width="150px"

        />

        <core:Title text="" />

         

        <Label

            id="_IDGenLabel3"

            text="Travel Mode"

            required="true"

             design="Bold"

        />

     

        <Select

            id="country1"

            selectedKey="{Country}"

            width="150px"

        >

            <items>

                <core:Item id="ui" text="select" />

                <core:Item id="_IDGenItem3" text="Flight" />

                <core:Item id="_IDGenItem4" text="Train" />

                <core:Item id="_IDGenItem5" text="Car" />

                <core:Item id="_IDGenItem6" text="Other" />

            </items>

        </Select>

        <Label text=""></Label>

        <Text id="_IDGenText3" text=""></Text>

        <Label text="Trip type" required="true" design="Bold"></Label>

        <Select

            id="country2"

            selectedKey="{Country}"

            width="150px"

        >

            <items>

                 <core:Item id="ui3" text="select" />

                <core:Item id="_IDGenItem7" text="One-way" />

                <core:Item id="_IDGenItem8" text="Round-Trip" />

            </items>

        </Select>

    </form:SimpleForm>

   

  </Shell>

 </Panel>

 <Toolbar id="_IDGenToolbar1" >

 <ToolbarSpacer id="_IDGenToolbarSpacer1" ></ToolbarSpacer>

 <Button id="_IDGenButton1" type="Emphasized" text ="Next" width="150px" press="onNext"></Button>

 </Toolbar>

 

</Page>

</mvc:View>