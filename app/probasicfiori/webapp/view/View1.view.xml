<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:svm="sap.ui.comp.smartvariants"
    controllerName="probasicfiori.controller.View1"
    displayBlock="true"
>
    <f:DynamicPage id="_IDGenDynamicPage11">
        <f:header>
            <f:DynamicPageHeader id="_IDGenDynamicPageHeader1">
                <f:content>
                    <fb:FilterBar
                        id="filterbar"
                        width="auto"
                        persistencyKey="myPersKey"
                        useToolbar="true"
                        search="onSearch"
                        filterChange=".onFilterChange"
                        afterVariantLoad=".onAfterVariantLoad"
                    >
                        <fb:filterGroupItems>
                            <fb:FilterGroupItem
                                id="_IDGenFilterGroupItem1"
                                name="Name"
                                label="Employee ID"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <ComboBox
                                        id="input1"
                                        class="sapUiSmallMarginBottom"
                                        name="Name"
                                        selectionChange=".onSelectionChange"
                                        items="{path: '/Travel', templateShareable: true }"
                                    >
                                        <core:Item
                                            id="_IDGenItem44"
                                            text="{empId_Empid}"
                                        />
                                    </ComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem
                                id="_IDGenFilterGroupItem4"
                                name="SupplierNames"
                                label="Travel status"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <Select
                                        id="input2"
                                        class="sapUiSmallMarginBottom"
                                        selectedKey="{travelStatus}"
                                        width="150px"
                                    >
                                        <items>
                                            <!-- <core:Item id="yi8" text="{travelStatus}" key="{travelStatus}"/> -->

                                            <core:Item
                                                id="yi1"
                                                text="Select"
                                            />

                                            <core:Item
                                                id="yi"
                                                text="APD"
                                            />

                                            <core:Item
                                                id="yi34"
                                                text="RJT"
                                            />

                                            <core:Item
                                                id="yi4Reject"
                                                text="INP"
                                            />
                                        </items>
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem
                                id="_IDGenFilterGroupItem2"
                                name="Category"
                                label="Start Date"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <DatePicker
                                        id="input3"
                                        change="handleChange"
                                        class="sapUiSmallMarginBottom"
                                        placeholder="Start Date"
                                        xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
                                        cd:w5g.dt.context="/Travel"
                                        valueFormat="{dateOfDeparture}"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem
                                id="_IDGenFilterGroupItem3"
                                name="SupplierName"
                                label="End Date"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <DatePicker
                                        id="input4"
                                        placeholder="End Date"
                                        change="handleChange"
                                        class="sapUiSmallMarginBottom"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                        </fb:filterGroupItems>
                    </fb:FilterBar>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>

        <f:content>
            <Table
                id="table"
                inset="false"
                items="{                          path: '/Travel'                      }"
            >
                <headerToolbar>
                    <OverflowToolbar id="_IDGenOverflowToolbar1">
                        <Title
                            id="_IDGenTitle1"
                            text="Travel Detail List"
                            level="H2"
                        />

                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />

                        <!-- <Label id="_IDGenLabel1" text="Multi selection modes" labelFor="idComboBoxSuccess"></Label> -->

                        <Button
                            id="button"
                            text="create"
                            press="onPress1"
                            type="Emphasized"
                        />
                    </OverflowToolbar>
                </headerToolbar>

                <columns>
                    <Column
                        id="_IDGenColumn14"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <Text
                            id="_IDGenText13"
                            text="Employee ID"
                        />
                    </Column>

                    <Column
                        id="_IDGenColumn1"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <Text
                            id="_IDGenText1"
                            text="Travel Id"
                        />
                    </Column>

                    <Column
                        id="_IDGenColumn2"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <Text
                            id="_IDGenText2"
                            text="Employee name"
                        />
                    </Column>

                    <Column
                        id="_IDGenColumn3"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <Text
                            id="_IDGenText3"
                            text="Origin"
                        />
                    </Column>

                    <Column
                        id="_IDGenColumn4"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <Text
                            id="_IDGenText4"
                            text="Destination"
                        />
                    </Column>

                    <Column
                        id="_IDGenColumn5"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <Text
                            id="_IDGenText5"
                            text="Status"
                        />
                    </Column>

                    <Column
                        id="_IDGenColumn6"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <Text
                            id="_IDGenText86"
                            text="Date of Departure"
                        />
                    </Column>

                    <Column
                        id="_IDGenColumn7"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <Text
                            id="_IDGenText87"
                            text="Date of Arrival"
                        />
                    </Column>
                </columns>

                <items>
                    <ColumnListItem
                        id="_IDGenColumnListItem1"
                        vAlign="Middle"
                        type="Navigation"
                        press="onpage4"
                    >
                        <cells>
                            <Text
                                id="_IDGenText45"
                                text="{empId_Empid}"
                            />

                            <Text
                                id="_IDGenText41"
                                text="{travelId}"
                            />

                            <Text
                                id="_IDGenText51"
                                text="{empName_Empid}"
                            />

                            <Text
                                id="_IDGenText6"
                                text="{origin}"
                            />

                            <Text
                                id="_IDGenText64"
                                text="{destination}"
                            />

                            <Text
                                id="_IDGenText65"
                                text="{travelStatus}"
                            />

                            <Text
                                id="_IDGenText66"
                                text="{dateOfDeparture}"
                            />

                            <Text
                                id="_IDGenText67"
                                text="{dateOfArrival}"
                            />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </f:content>
    </f:DynamicPage>
</mvc:View>
